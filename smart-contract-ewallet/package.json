  "jest": {
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/node_modules/react-native-keyboard-aware-scroll-view",
      "/node_modules/react-native-system-setting"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-native-system-setting|native-base|react-native-keyboard-aware-scroll-view|react-navigation|bugsnag-react-native|static-container|native-base-shoutem-theme|@shoutem/animation|@shoutem/ui|tcomb-form-native)"
    ],
    "globals": {
      "__DEV__": true
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx?$": "babel-jest"
    }
