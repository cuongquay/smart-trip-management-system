{
  "name": "tripcontract",
  "version": "2.0.0",
  "license": "MIT",
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --single-quote --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "startrs": "npm start -- --reset-cache",
    "test": "jest",
    "flow": "node_modules/.bin/flow",
    "checkdeps": "ncu --packageFile package.json",
    "i5s": "react-native run-ios --simulator=\"iPhone 5s\"",
    "i6": "react-native run-ios --simulator=\"iPhone 6\"",
    "iX": "react-native run-ios --simulator=\"iPhone X\"",
    "a": "react-native run-android",
    "ar": "react-native run-android --variant=\"release\"",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "clean:android-win": "cd android/ && gradlew clean && cd ..",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:buildRelease": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "precommit": "lint-staged && npm run git-hook",
    "prepush": "npm run git-hook",
    "postinstall": "npm run rn-camera-no-gvm && chmod +x scripts/websocket-fix.sh && scripts/websocket-fix.sh",
    "git-hook": "echo 'Originally npm test -s'",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "eslint-fix": "eslint --fix",
    "lint-check": "lint-staged",
    "check-s3-location": "aws s3api get-bucket-location --bucket apps.tripcontract.org",
    "deploy-ios": "cd ios && fastlane build_s3 && cd ..",
    "deploy-android": "cd android && fastlane build_s3 && cd ..",
    "deploy": "npm run deploy-ios && npm run deploy-android",
    "release-ios": "cd ios && fastlane release && cd ..",
    "release-android": "cd android && fastlane release && cd ..",
    "release": "npm run release-ios && npm run release-android",
    "rel": "appcenter codepush release-react -a doko/Trip|Contract-Flash",
    "rn-camera-no-gvm": "echo \"Removing Face Detector\" && cp node_modules/react-native-camera/postinstall_project/projectWithoutFaceDetection.pbxproj node_modules/react-native-camera/ios/RNCamera.xcodeproj/project.pbxproj"
  },
